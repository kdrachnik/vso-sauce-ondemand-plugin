{
    "manifestVersion": 1,
    "id": "saucelabs-tasks",
    "name": "Sauce Labs Plugin for VSO",
    "version": "0.1.3",
    "publisher": "saucelabs",
    "description": "Tools for building with Sauce labs. Includes two build tasks.",
    "categories": [
        "Build and release"
    ],
    "content": {
      "details": {
        "path": "overview.md"
      }
    },
    "screenshots": [
      {
        "path": "images/saucelabs-icon.png"
      }
    ],
    "icons": {
      "default": "images/saucelabs-icon.png"
    },
    "files": [
      { "path": "images", "addressable": true },
      { "path": "sod-main" },
      { "path": "sod-stop-sc" }
    ],
    "contributions": [
        {
            "id": "sod-main",
            "type": "ms.vss-distributed-task.task",
            "targets": [ "ms.vss-distributed-task.tasks" ],
            "properties": { "name": "sod-main" }
        },
        {
            "id": "sod-stop-sc",
            "type": "ms.vss-distributed-task.task",
            "targets": [ "ms.vss-distributed-task.tasks" ],
            "properties": { "name": "sod-stop-sc" }
        }
    ]
}
