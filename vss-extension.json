{
    "manifestVersion": 1,
    "id": "saucelabs-tasks",
    "name": "Sauce Labs Plugin for VSO",
    "version": "0.1.3",
    "publisher": "saucelabs",
    "description": "Tools for building with Sauce labs. Includes two build tasks.",
    "categories": [ "Test" ],
    "branding": {
      "color": "#00AFAA",
      "theme": "dark"
    },
    "content": {
      "details": {
        "path": "overview.md"
      }
    },
    "links": {
      "home": { "uri": "https://saucelabs.com" },
      "gettingstarted": { "uri": "https://saucelabs.com/signup" },
      "learn": { "uri": "https://docs.saucelabs.com/" },
      "issues": { "uri": "https://support.saucelabs.com/" },
      "support": { "uri": "https://support.saucelabs.com" },
      "repository": { "uri": "https://github.com/saucelabs/vso-sauce-ondemand-plugin" }
    },
    "screenshots": [
      { "path": "images/screenshot1.png" }
    ],
    "icons": {
      "default": "images/saucelabs-icon.png"
    },
    "files": [
      { "path": "images", "addressable": true },
      { "path": "sod-main" },
      { "path": "sod-stop-sc" }
    ],
    "contributions": [
        {
            "id": "sod-main",
            "type": "ms.vss-distributed-task.task",
            "targets": [ "ms.vss-distributed-task.tasks" ],
            "properties": { "name": "sod-main" }
        },
        {
            "id": "sod-stop-sc",
            "type": "ms.vss-distributed-task.task",
            "targets": [ "ms.vss-distributed-task.tasks" ],
            "properties": { "name": "sod-stop-sc" }
        }
    ]
}
